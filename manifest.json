{
    "manifest_version": 3,
    "name": "Tampermonkey Script",
    "version": "1.0",
    "content_scripts": [{
      "matches": ["https://development.instructure.com/*"],
      "js": ["tampermonkey_scripts.js"],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }],
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "https://development.instructure.com/*",
      "https://*/*",
      "http://*/*"
    ]
  }
  